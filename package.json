{
  "name": "@dingo/source",
  "version": "0.0.0",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "nx run-many -t build -p @dingo/frontend @dingo/backend",
    "build:backend": "nx build @dingo/backend",
    "build:frontend": "nx build @dingo/frontend",
    "dev": "nx run-many -t dev -p @dingo/frontend @dingo/backend --output-style=stream",
    "dev:backend": "nx dev @dingo/backend --output-style=stream",
    "dev:frontend": "nx dev @dingo/frontend",
    "lint": "nx run-many -t lint",
    "lint:backend": "nx lint @dingo/backend",
    "lint:frontend": "nx lint @dingo/frontend",
    "format": "nx run-many -t format",
    "format:frontend": "prettier --write apps/frontend",
    "format:backend": "prettier --write apps/backend",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "20.0.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/compat": "1.4.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.36.0",
    "@nx/esbuild": "21.6.3",
    "@nx/eslint": "21.6.3",
    "@nx/eslint-plugin": "21.6.3",
    "@nx/express": "21.5.3",
    "@nx/js": "21.6.3",
    "@nx/node": "^21.6.3",
    "@nx/web": "21.5.3",
    "@nx/webpack": "21.5.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.17",
    "@svgr/webpack": "8.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.5.2",
    "autoprefixer": "10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.0.1",
    "esbuild": "0.25.10",
    "eslint": "9.36.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "jest-environment-node": "30.1.2",
    "jest-util": "30.0.5",
    "nx": "21.6.3",
    "postcss": "8.5.6",
    "prettier-plugin-tailwindcss": "0.6.14",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.44.1",
    "webpack-cli": "5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "axios": "1.12.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "2.8.5",
    "dotenv": "17.2.2",
    "express": "4.21.2",
    "helmet": "8.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.1.11"
  }
}
